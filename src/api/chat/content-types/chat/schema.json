{
  "kind": "collectionType",
  "collectionName": "chats",
  "info": {
    "singularName": "chat",
    "pluralName": "chats",
    "displayName": "Chat"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "type": {
      "type": "enumeration",
      "enum": [
        "group",
        "singles"
      ]
    },
    "created_chat_by": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "chat"
    },
    "message": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::message.message",
      "inversedBy": "chat_id"
    }
  }
}
